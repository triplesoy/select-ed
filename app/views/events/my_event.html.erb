<div class="container mx-auto mt-4">
  <h1>My events</h1>
<div class="container mx-auto mt-4">
  <div class="row">
    <% @my_events.each do |event| %>
      <div class="col-lg-3 my-3">
        <div class="card" style="width: 18rem; height: 22rem;">
          <div class="card-body position-relative">
            <h5 class="card-title"><%= event.community.title %></h5>
            <h6 class="card-subtitle mb-2 text-muted"></h6>
            <p class="card-text position-absolute bottom-0 start-2 fw-bold">
            <%= link_to "Details", community_event_path(event.community, event), class: "btn btn-outline-dark position-absolute bottom-0 end-0 m-2" %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
