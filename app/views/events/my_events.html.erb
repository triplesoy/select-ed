<div class="container mx-auto mt-4">
  <h1>My events</h1>
  <div class="container mx-auto mt-4">
    <div class="row">
      <% @my_events.each do |event| %>
        <div class="col-lg-3 my-3">
          <div class="card border border-2 border-gray shadow" style="width: 18rem; height: 22rem;">
            <div class="card-body position-relative">
              <h5 class="card-title"><%= event.title %></h5>
              <h6 class="card-subtitle mb-2 text-muted"></h6>
              <a href="<%= community_event_path(event.community, event) %>" class="btn btn-outline-primary position-absolute bottom-0 end-0 mb-3 me-3">Details</a>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
